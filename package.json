{
  "scripts": {
    "build": "npm run cp-libs && tsc && npm run cp-sw",
    "cp-libs": "npm run cp-scop",
    "cp-scop": "mkdir -p bin/ed25519; rsync -avc --exclude='*.json' node_modules/ed25519.wasm/* bin/ed25519",
    "cp-sw": "cp -f bin/sw.js sw.js",
    "webfsd": "killall -q webfsd; webfsd -F -l - -p 8080 -r . -f index.html"
  },
  "devDependencies": {
    "@types/ol": "5.3.5",
    "typescript": "3.7.2"
  },
  "dependencies": {
    "ed25519.wasm": "1.0.3"
  }
}
